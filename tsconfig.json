{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"sourceMap": true,
		"jsx": "react"
	},
	"compileOnSave": false,
	"buildOnSave": false,
	"exclude": [
		"node_modules"
	],
	"filesGlob": [
		"./App/**/*.ts",
		"./App/**/*.tsx"
	],
	"files": [
		"./App/ActionCreators/TaskActionCreator.ts",
		"./App/Actions/IAction.ts",
		"./App/Actions/TaskAddAction.ts",
		"./App/Actions/TaskDeleteAction.ts",
		"./App/Actions/TaskDeleteAllAction.ts",
		"./App/Actions/TaskToggleAction.ts",
		"./App/Dispatcher/Dispatcher.ts",
		"./App/Log/ActionLogEntry.ts",
		"./App/Log/ActionLogger.ts",
		"./App/Stores/__tests__/TaskStore-test.ts",
		"./App/Stores/BaseStore.ts",
		"./App/Stores/TaskStore.ts",
		"./App/App.tsx",
		"./App/Components/ContentPage/__tests__/ContentPage-test.tsx",
		"./App/Components/ContentPage/ContentPage.tsx",
		"./App/Components/Tasks/__tests__/TaskSummaryComponent-test.tsx",
		"./App/Components/Tasks/TaskComponent.tsx",
		"./App/Components/Tasks/TaskFormComponent.tsx",
		"./App/Components/Tasks/TaskListComponent.tsx",
		"./App/Components/Tasks/TaskSummaryComponent.tsx",
		"./App/Index.tsx"
	],
	"atom": {
		"rewriteTsconfig": true
	}
}
